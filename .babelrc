{
  "presets": [
    ["@babel/env", {
      "targets": {
        "node": "6.11.5",
        "edge": "17",
        "firefox": "60",
        "chrome": "67",
        "safari": "11.1"
      },
      "useBuiltIns": "usage"
    }],
    "@babel/preset-react",
    ["@babel/preset-typescript", {
      "isTSX": true,
      "allExtensions": true
    }]
  ],
  "plugins": [
    ["styled-components", { "ssr": true, "displayName": true }]
  ]
}